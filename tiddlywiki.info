{
	"description": "Testing CYOA",
	"plugins": [
		"tiddlywiki/jasmine",
		"tiddlywiki/xmldom",
		"tiddlywiki/highlight",
		"felixhayashi/tiddlymap",
		"felixhayashi/vis",
		"felixhayashi/hotzone",
		"felixhayashi/topstoryview",
		"flibbles/relink",
		"slaymaker1907/browser-nativesaver"
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"test": [
			"--output", "./docs/",
			"--rendertiddler","$:/core/save/all","test.html","text/plain", "", "publishFilter", "-[prefix[$:/plugins/]plugin-type[plugin]!prefix[$:/plugins/tiddlywiki/]!prefix[$:/plugins/mythos/]!match[$:/plugins/tiddlywiki/highlight]]"
		],
		"dist": [
			"--output", "./docs/",
			"--setfield", "$:/plugins/mythos/cyoa", "created", "Templates/now", "text/plain",
			"--rendertiddler","$:/core/save/all","index.html","text/plain", "", "publishFilter", "-[prefix[$:/plugins/tiddlywiki/]!match[$:/plugins/tiddlywiki/highlight]] -[prefix[test/]] -[[cyoa.js]] -[[$:/config/Plugins/Disabled/$:/plugins/flibbles/relink]]"
		],
		"serve": [
			"--setfield", "$:/config/Plugins/Disabled/$:/plugins/flibbles/relink", "text", "", "",
			"--listen"
		]
	}
}
