var types=cyoa.stateClasses,
	data=<$text text=<<cyoa-data>> />;
cyoa.stackVariable="<$transclude tiddler="$:/config/mythos/cyoa/stack">stack</$transclude>";
cyoa.vars = Object.create(null);
<$list
	filter="[[$:/tags/cyoa/Type]tagging[]]"
><$let
	variable={{{ [all[current]cyoa:variable[cyoa.key]] }}}
>declare(cyoa.vars,"<<variable>>",types.{{!!cyoa.handler}},{data: data.<<variable>>});
</$let></$list>
declare(cyoa.vars,cyoa.stackVariable,types.stack);
